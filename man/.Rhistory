library(bkcde)
set.seed(42)
n <- 100
x <- runif(n,-.25,.25)
y <- rbeta(n,1+x,1.5+x)
f.yx <- bkcde(x=x, y=y)
x.eval <- c(0,.1)
y.eval <- c(.25,0.5)
## Predicted density values
predict(f.yx, data.frame(x=x.eval, y=y.eval))
## True density values
dbeta(y.eval, 1+x.eval, 1.5+x.eval)
plot(f.yx)
options(digits=2)
predict(f.yx, data.frame(x=x.eval, y=y.eval))
## True density values
dbeta(y.eval, 1+x.eval, 1.5+x.eval)
options(digits=4)
predict(f.yx, data.frame(x=x.eval, y=y.eval))
## True density values
dbeta(y.eval, 1+x.eval, 1.5+x.eval)
options(digits=3)
predict(f.yx, data.frame(x=x.eval, y=y.eval))
## True density values
dbeta(y.eval, 1+x.eval, 1.5+x.eval)
